CHIP RegisterHiLo8
{
    IN in[4], loadLo, loadHi;
    OUT out[8];

    PARTS:

	Bit(in=in[0],out=out[0],load=loadLo);
	Bit(in=in[1],out=out[1],load=loadLo);
	Bit(in=in[2],out=out[2],load=loadLo);
	Bit(in=in[3],out=out[3],load=loadLo);
	
	Bit(in=in[0],out=out[4],load=loadHi);
	Bit(in=in[1],out=out[5],load=loadHi);
	Bit(in=in[2],out=out[6],load=loadHi);
	Bit(in=in[3],out=out[7],load=loadHi);
	
}
